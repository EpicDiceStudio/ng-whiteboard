{
  "$schema": "..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "sourceRoot": "projects/ng-whiteboard/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/projects/ng-whiteboard",
        "main": "projects/ng-whiteboard/src/index.ts",
        "tsConfig": "projects/ng-whiteboard/tsconfig.lib.json",
        "assets": ["projects/ng-whiteboard/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["projects/ng-whiteboard/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/projects/ng-whiteboard"],
      "options": {
        "jestConfig": "projects/ng-whiteboard/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
