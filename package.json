  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EpicDiceStudio/ng-whiteboard.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
